.container{
	padding-top:107rpx;
}
.tag-bar{
	padding-top:27rpx;height:80rpx;background:#fff;border-bottom:1px solid #ddd;
	position:fixed;left:0;top:0;width:100%;z-index:2;
}
.tag-bar .tag{
	float:left;width:20%;height:80rpx;line-height:80rpx;text-align:center;font-size:30rpx;
	color:#333;
}
.tag-bar .cur{
	color:#46b37a;
}

.item-group{
	background:#fff;padding:27rpx;margin-bottom:27rpx;
}
.item-group .order-numb{
	height:42rpx;line-height:42rpx;font-size:28rpx;color:#666;
}
.item-group .order-numb::before{
	content:attr(data-before);color:#fff;padding:4rpx 8rpx;background:#f28e31;margin-right:12rpx;
	border-radius:4px;overflow:hidden;
}
.item-group .state_3::before{
	background:#29c661;
}
.item-group .state_4::before{
	background:#999;
}
.item-group .state_-1::before{
	background:#ed4636;
}

.item-group .order-date{
	height:36rpx;line-height:36rpx;font-size:24rpx;color:#999;padding-top:6rpx;
}
.item-group .order-note{
	height:60rpx;line-height:60rpx;font-size:34rpx;font-weight:bold;color:#333;padding-top:12rpx;
}

.item-group .order-info-bar{
	height:60rpx;line-height:60rpx;font-size:28rpx;color:#666;
}
.item-group .order-info-bar text.name{float:left;}
.item-group .order-info-bar text.tel{float:right;padding-right:60rpx;}
.item-group .detail-bar{
	
}
.item-group .detail-bar .tag-list{
	overflow:hidden;padding-right:60rpx;
}
.item-group .detail-bar .tag-list text{
	float:left;background:#7accb1;height:48rpx;line-height:48rpx;padding:0 12rpx;font-size:26rpx;color:#fff;
	margin:6rpx;border-radius:3px;
}
.item-group .detail-bar .info-text{
	text-align:right;padding:0 60rpx 0 0;font-size:24rpx;color:#999;
	height:32rpx;line-height:32rpx;
}
.item-group .detail-bar .info-text text{
	color:#ed4636;
}

.item-group .btn-bar{
	min-height:60rpx;padding:12rpx 0;overflow:hidden;border-top:1px solid #eee;margin-top:30rpx;
}
.item-group .btn-bar view{
	float:right;height:60rpx;line-height:60rpx;text-align:center;font-size:30rpx;color:#b7b06f;
	border-right:1px solid #ddd;padding:0 20rpx;
}
.item-group .btn-bar .sc{color:#999;}
.item-group .btn-bar view:first-child{
	border-right:none;
}
.item-group .btn-bar view.pj_star{
	
}

.item-group .btn-bar view.pj_star .star_1,
.item-group .btn-bar view.pj_star .star_2{
	float:left;width:33rpx;height:31rpx;margin:12rpx 5rpx 0 0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:33rpx 31rpx;border:none;padding:0;
}
.item-group .btn-bar view.pj_star .star_2{
	background:url(data:image/png;base64,) no-repeat;
	background-size:33rpx 31rpx;
}










