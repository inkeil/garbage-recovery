
.top-bar{
	height:366rpx;position:relative;
}
.top-bar .user-info{
	background:#41bb86;padding:22rpx 27rpx 0;height:255rpx;
}
.top-bar .user-info .avatar-group{
	float:left;margin-left:27rpx;width:120rpx;
}
.top-bar .user-info .avatar-group .avatar{
	width:110rpx;height:110rpx;border:5rpx solid #fff;overflow:hidden;
	border-radius:50%;
}
.top-bar .user-info .avatar-group .avatar image{
	width:110rpx;height:110rpx;
}
.top-bar .user-info .avatar-group .level{
	height:52rpx;line-height:52rpx;color:#fff;font-size:30rpx;text-align:center;
}
.top-bar .user-info .user-group{
	float:left;margin-left:27rpx;width:382rpx;
}
.top-bar .user-info .user-group .name{
	height:56rpx;line-height:56rpx;font-size:30rpx;color:#fff;
}
.top-bar .user-info .user-group .star-level{
	height:56rpx;line-height:56rpx;
}
.top-bar .user-info .user-group .star-level .star_1,
.top-bar .user-info .user-group .star-level .star_2{
	float:left;width:33rpx;height:31rpx;margin:12rpx 10rpx 0 0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:33rpx 31rpx;
}
.top-bar .user-info .user-group .star-level .star_2{
	background:url(data:image/png;base64,) no-repeat;
	background-size:33rpx 31rpx;
}
.top-bar .user-info .user-group .scores{
	height:52rpx;line-height:52rpx;color:#fff;font-size:30rpx;margin-top:8rpx;
}
.top-bar .user-info .message-btn{
	float:right;font-size:26rpx;color:#bde3cd;height:56rpx;line-height:56rpx;padding-right:27rpx;
}




.top-bar .achievement{
	position:absolute;background:#fff;left:50%;bottom:0;
	border-radius:18rpx;height:160rpx;width:696rpx;margin-left:-348rpx;
	box-shadow:0 0 20rpx rgba(0,0,0,.3);
}
.top-bar .achievement .my-icon{
	float:left;width:105rpx;height:108rpx;margin:26rpx 0;border-right:1px solid #ddd;
	padding:0 27rpx;
}
.top-bar .achievement .my-icon::before{
	content:"";display:block;width:53rpx;height:68rpx;margin:0 auto;
	background:url(data:image/png;base64,) no-repeat;
	background-size:53rpx 68rpx;
}
.top-bar .achievement .my-icon::after{
	content:attr(data-after);display:block;height:40rpx;text-align:center;line-height:40rpx;font-size:24rpx;color:#f0930f;
}

.top-bar .achievement .text-info{
	float:left;height:108rpx;margin:26rpx 0 26rpx 49rpx;
}
.top-bar .achievement .text-info .text{
	height:54rpx;line-height:54rpx;font-size:24rpx;color:#999;
}
.top-bar .achievement .text-info .text text{
	font-size:30rpx;color:#ef2e45;padding:5rpx;font-weight:600;line-height:54rpx;
}

.month-income{
	margin:27rpx 27rpx 0;background:#fff;border-radius:18rpx;padding:0 0 30rpx;
}
.month-income::before{
	content:attr(data-tit);height:80rpx;line-height:80rpx;font-size:28rpx;color:#666;padding:0 40rpx;
}

.month-income .img{
	margin:0 auto;display:block;
}
.list-box{
	margin:27rpx;
}
.list-box .item{
	height:90rpx;background:#fff;border-radius:18rpx;margin-bottom:17rpx;position:relative;
}
.list-box .item::before{
	content:attr(data-tit);float:left;width:319rpx;line-height:90rpx;text-indent:40rpx;
	color:#666;font-size:30rpx;
}
.list-box .item::after{
	content:"";display:block;width:18rpx;height:33rpx;position:absolute;right:27rpx;top:50%;margin-top:-16rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:18rpx 33rpx;
}

.list-box .item .money,
.list-box .item .text{
	float:right;padding-right:84rpx;line-height:90rpx;font-size:30rpx;color:#ed5f2b;width:200rpx;text-align:right;
}
.list-box .item .text{
	font-size:24rpx;color:#999;
}

.copy-box{
text-align: center;
padding:4%;
}
.copy-right{
font-size:12px;
color:#ccc;
}










