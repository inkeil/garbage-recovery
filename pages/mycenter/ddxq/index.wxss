page{background:#fff;}
.order-add-info{
	background:#fff;padding:27rpx 47rpx;border-bottom:1px solid #ddd;
}
.order-add-info .tag-bar{
	height:100rpx;overflow:hidden;
}
.order-add-info .tag-bar view{
	float:left;height:42rpx;line-height:42rpx;font-size:30rpx;margin:19rpx 60rpx 19rpx 0;color:#333;padding:10rpx;
	color:#b7b06f;
}
.order-add-info .tag-bar view[class^="state_"]{
	background:#f28e31;color:#fff;border-radius:6px;overflow:hidden;font-size:30rpx;
}
.order-add-info .tag-bar view.state_3{
	background:#29c661;
}
.order-add-info .tag-bar view.state_4{
	background:#999;
}
.order-add-info .tag-bar view.state_-1{
	background:#ed4636;
}

.order-add-info .tag-bar view.wpzh{
	padding-right:0;margin-right:0;
}

.order-add-info .tag-bar view.pj_star{
	float:left;margin-top:7rpx;
}

.order-add-info .tag-bar .pj_star .star_1,
.order-add-info .tag-bar .pj_star .star_2{
	float:left;width:33rpx;height:31rpx;margin:12rpx 2rpx 0 0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:33rpx 31rpx;
}
.order-add-info .tag-bar .pj_star .star_2{
	background:url(data:image/png;base64,) no-repeat;
	background-size:33rpx 31rpx;
}

.order-add-info .address{
	min-height:40rpx;line-height:40rpx;padding:16rpx 0;font-size:28rpx;color:#999;
}
.order-add-info .userinfo{
	height:40rpx;line-height:40rpx;padding:16rpx 0;font-size:28rpx;color:#999;
}
.order-add-info .userinfo text{
	padding-right:40rpx;
}
.order-add-info .userinfo text.tel{padding:0;}
.order-add-info .takeoversinfo{
	min-height:40rpx;line-height:40rpx;padding:16rpx 0;font-size:28rpx;color:#60bf9f;
}
.order-add-info .takeoversinfo text{padding-right:40rpx;}
.order-add-info .takeoversinfo text.tel{padding:0;}
.order-add-info .order-note{
	min-height:40rpx;line-height:40rpx;padding:16rpx 0;font-size:32rpx;font-weight:bold;color:#60bf9f;
}

.order-res{
	background:#fff;padding:27rpx;border-bottom:1px solid #ddd;
}
.order-res .item-group{
	height:42rpx;padding:20rpx;border-bottom:1px solid #eee;overflow:hidden;
}
.order-res .item-group view{
	float:left;line-height:42rpx;height:42rpx;font-size:30rpx;color:#777;overflow:hidden;
	padding:0 20rpx;
}
.order-res .item-group .tit{
	color:#333;width:180rpx;padding-left:0;text-align:right;
}
.order-res .item-group .level{
	padding:6rpx 30rpx 6rpx 0;width:30rpx;height:30rpx;
}
.order-res .item-group .level::before{
	content:"";float:left;width:30rpx;height:30rpx;
}
.order-res .item-group .numb{
	width:100rpx;
}
.order-res .item-group .price{
	width:200rpx;
}
.order-res .item-group .lie::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:30rpx 30rpx;
}
.order-res .item-group .zhong::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:30rpx 30rpx;
}
.order-res .item-group .you::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:30rpx 30rpx;
}

.order-res .total-bar{
	height:42rpx;padding:20rpx 20rpx 0;overflow:hidden;font-size:30rpx;color:#999;text-align:right;
}
.order-res .total-bar text{color:#ed4636;}

.order-info-bar{
	padding:17rpx 47rpx 40rpx;background:#fff;
}
.order-info-bar .text{
	height:42rpx;padding:10rpx;overflow:hidden;
	font-size:30rpx;color:#666;
}
.order-info-bar .after-sales{
	height:50rpx;line-height:50rpx;padding:20rpx;font-size:38rpx;color:#e4516d;
	/*41*/
}
.order-info-bar .after-sales::before{
	content:"";width:41rpx;height:50rpx;float:left;
	background:url(data:image/png;base64,) no-repeat;
	background-size:41rpx 50rpx;
	margin-right:20rpx;
}








