.top-bar{
	height:342rpx;
	background:#41bb86;
}
.top-bar .text-info{
	padding-left:204rpx;width:310rpx;height:112rpx;
	padding-top:32rpx;position:relative;color:#fff;float:left;
}
.top-bar .text-info::before{
	content:attr(data-tit);display:block;height:46rpx;line-height:46rpx;font-size:30rpx;
}
.top-bar .text-info::after{
	content:"Â¥";display:block;width:92rpx;height:92rpx;border:10rpx solid #efcc25;background:#fbdf64;
	color:#efcc25;font-size:80rpx;text-align:center;line-height:92rpx;border-radius:50%;font-weight:bold;
	position:absolute;left:70rpx;top:32rpx;
}
.top-bar .text-info text{
	font-size:48rpx;height:66rpx;line-height:62rpx;display:block;
}
.top-bar .tixian-btn{
	height:56rpx;line-height:56rpx;color:#fff;float:right;margin-top:32rpx;
	font-size:36rpx;padding-right:60rpx;font-weight:500;
}
.top-bar .tixian-btn::before{
	content:"";float:left;margin:11rpx 10rpx 0 0;
	width:39rpx;height:36rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:39rpx 36rpx;
}

.list-box{
	margin-top:-146rpx;padding:0 27rpx;
}

.list-box .tag-bar{
	height:84rpx;
}
.list-box .tag-bar .tag,
.list-box .tag-bar .tag1{
	float:left;width:204rpx;height:84rpx;line-height:84rpx;
	font-size:36rpx;color:#a2e1c0;text-align:center;
}
.list-box .tag-bar .tag1{
	width:284rpx;
}
.list-box .tag-bar .cur{color:#fff;}

.list-box .item-wrap{
	background:#fff;border-radius:18rpx;min-height:128rpx;
	box-shadow:0 0 20rpx rgba(0,0,0,.3);
}

.list-box .item-wrap .item-group{
	height:86rpx;overflow:hidden;padding:22rpx 0;border-bottom:1px solid #ddd;position:relative;
}
.list-box .item-wrap .item-group:last-child{
	border-bottom:none;
}
.list-box .item-wrap .item-group .texts{
	width:300rpx;height:50rpx;line-height:50rpx;color:#333;font-size:30rpx;text-indent:27rpx;
}
.list-box .item-wrap .item-group .times{
	height:36rpx;line-height:36rpx;font-size:20rpx;color:#999;width:300rpx;text-indent:27rpx;
}
.list-box .item-wrap .item-group .money{
	position:absolute;
	display:block;width:300rpx;height:86rpx;line-height:86rpx;font-size:36rpx;color:#666;
	right:27rpx;top:22rpx;text-align:right;
}
.list-box .item-wrap .item-group .orange{color:#f0901f;}



.tixian-box{
	position:fixed;left:0;top:0;width:100%;height:100%;background:rgba(0,0,0,0.3);
}
.tixian-box .con{
	position:absolute;left:0;bottom:0;background:#fff;width:100%;height:80%;
}
.tixian-box .con::before{
	content:attr(data-before);display:block;height:30rpx;line-height:30rpx;font-size:32rpx;color:#999;text-indent:32rpx;
}

.tixian-box .con .input-box,
.tixian-box .con .btn-bar{
	min-height:72rpx;overflow:hidden;
	padding:18rpx 32rpx 0;
}

.tixian-box .con .input-box input{
	box-sizing:border-box;float:left;width:100%;height:70rpx;line-height:70rpx;border:1px solid #ddd;border-radius:4px;background:#fbfbfb;
	padding:0 10rpx;font-size:26rpx;color:#666;
}

.tixian-box .con .tishi{
	font-size:26rpx;color:#999;text-align:right;line-height:40rpx;height:40rpx;padding:20rpx 32rpx 0;
}
.tixian-box .con .tishi_1{
	font-size:32rpx;color:#666;text-align:right;line-height:50rpx;height:50rpx;padding:0 32rpx;
}
.tixian-box .con .btn-bar{padding-top:30rpx;}
.tixian-box .con .btn-bar .cancel-btn{
	float:right;width:40%;height:80rpx;line-height:80rpx;background:#ddd;border-radius:6px;color:#fff;
	font-size:32rpx;text-align:center;
}
.tixian-box .con .submit-btn{
	float:left;width:50%;height:80rpx;line-height:80rpx;background:#50be84;border-radius:6px;color:#fff;
	font-size:32rpx;text-align:center;
}













