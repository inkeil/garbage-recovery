.my-address{
	height:50rpx;padding-top:37rpx;
}
.my-address .title{
	height:50rpx;line-height:50rpx;padding-left:56rpx;float:left;font-size:34rpx;color:#333;
}
.my-address .add-btn,
.my-address .address-save-btn{
	float:right;margin-right:56rpx;height:50rpx;line-height:50rpx;border-radius:4px;overflow:hidden;
	background:#6ac4a6 url(data:image/png;base64,) no-repeat;
	background-size:30rpx 28rpx;background-position:13rpx 11rpx;
	width:152rpx;font-size:24rpx;color:#fff;text-indent:45rpx;
}
.my-address .address-save-btn{
	background:#6ac4a6 url(data:image/png;base64,) no-repeat;
	background-size:30rpx 28rpx;background-position:13rpx 11rpx;
}
.my-address .address-cancel-btn{
	float:right;margin-right:10rpx;height:50rpx;line-height:50rpx;border-radius:4px;overflow:hidden;
	font-size:24rpx;color:#fff;background:#ccc;padding:0 12rpx;
}
.address-list{
	padding:0 27rpx;
}
.address-list .item{
	margin-top:20rpx;
	background:#fff;border-radius:18rpx;overflow:hidden;
	padding:16rpx 27rpx 0;
}
.address-list .default-item{
	box-shadow: 0 0 20rpx #a8e6e2;border:1px solid #6ac4a6;
}
.address-list .item .contact{
	padding-bottom:14rpx;height:32rpx;line-height:32rpx;font-size:24rpx;color:#999;
}
.address-list .item .contact::before{
	content:"";float:left;width:31rpx;height:31rpx;margin-right:12rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:31rpx 31rpx;
}
.address-list .default-item .contact{
	color:#6ac4a6;
}
.address-list .default-item .contact::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:31rpx 31rpx;
}

.address-list .item .info{
	min-height:84rpx;padding:22rpx 20rpx 22rpx 0;border-top:1px solid #ddd;border-bottom:1px solid #ddd;
	position:relative;
}

.address-list .item .info view{
	min-height:42rpx;line-height:42rpx;color:#999;
	font-size:24rpx;position:relative;padding-left:75rpx;
}

.address-list .default-item .info view{
	color:#333;
}

.address-list .item .info view:before{
	content:attr(data-before);display:block;height:100%;position:absolute;left:0;top:0;
	line-height:42rpx;
}
.address-list .item .seting-bar{
	height:40rpx;padding:22rpx 8rpx;font-size:24rpx;color:#999;
}
.address-list .default-item .seting-bar{
	color:#666;
}

.address-list .item .seting-bar .edit-btn{
	float:left;margin-right:34rpx;height:40rpx;line-height:40rpx;
}
.address-list .item .seting-bar .edit-btn::before{
	content:"";float:left;width:27rpx;height:24rpx;margin:8rpx 8rpx 0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:27rpx 24rpx;
}
.address-list .item .seting-bar .delete-btn{
	float:left;height:40rpx;line-height:40rpx;
}
.address-list .item .seting-bar .delete-btn::before{
	content:"";float:left;width:24rpx;height:24rpx;margin:8rpx 8rpx 0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:24rpx 24rpx;
}
.address-list .item .seting-bar .radio-btn,
.address-list .item .seting-bar .default-text{
	float:right;height:40rpx;line-height:40rpx;position:relative;
}
.default-text{
	color:#6ac4a6;
}
.address-list .item .seting-bar .radio-btn::before{
	content:"";float:left;width:80rpx;height:40rpx;border-radius:40rpx;background:#ccc;
	margin-right:12rpx;
}
.address-list .item .seting-bar .radio-btn::after{
	content:"";position:absolute;display:block;width:36rpx;height:36rpx;border-radius:50%;background:#fff;
	left:2rpx;top:2rpx;
}

.address-list .default-item .seting-bar .radio-btn::before{
	background:#6ac4a6;
}
.address-list .default-item .seting-bar .radio-btn::after{
	left:44rpx;
}
.address-list .btn-bar{
	height:70rpx;padding:27rpx 0 0;
}
.address-list .btn-bar .show-more-btn,
.address-list .btn-bar .hidden-more-btn{
	float:left;height:70rpx;line-height:70rpx;width:205rpx;font-size:28rpx;color:#fff;
	border-radius:4px;background:#ccc;
}
.address-list .btn-bar .show-more-btn::before,
.address-list .btn-bar .hidden-more-btn::before{
	content:"";float:left;width:28rpx;height:30rpx;margin:20rpx 10rpx 0 20rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:28rpx 30rpx;
}
.address-list .btn-bar .hidden-more-btn::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:28rpx 30rpx;
}

.address-list .btn-bar .submit-address-btn{
	width:396rpx;height:70rpx;line-height:70rpx;background:#6ac4a6;border-radius:4px;color:#fff;text-align:center;
	float:right;letter-spacing:30rpx;font-size:30rpx;text-indent:30rpx;
}

.address-edit{
	margin:20rpx 27rpx;background:#fff;border-radius:18rpx;padding:22rpx 27rpx;
}
.address-edit .input-box,
.address-edit .area-select-bar,
.address-edit .default-address-seting{
	height:72rpx;overflow:hidden;
	padding:18rpx 0;
}
.address-edit .input-box::before,
.address-edit .area-select-bar::before{
	content:attr(data-tit);float:left;line-height:72rpx;font-size:26rpx;color:#999;
	text-align:right;width:150rpx;
}
.address-edit .input-box input{
	float:left;width:470rpx;height:70rpx;line-height:70rpx;border:1px solid #ddd;border-radius:4px;background:#fbfbfb;
	padding:0 10rpx;font-size:26rpx;color:#666;
}
.address-edit .area-select-bar .area-text-box{
	float:left;font-size:26rpx;color:#666;line-height:72rpx;height:72rpx;padding-left:10rpx;
	width:470rpx;
}
.address-edit .area-select-bar .area-text-box::after{
	content:"";float:right;width:18rpx;height:33rpx;margin:19rpx 10rpx 0 0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:18rpx 33rpx;
}
.address-edit .default-address-seting::before{
	content:attr(data-tit);float:left;line-height:72rpx;font-size:26rpx;color:#999;
	text-align:right;margin-left:10rpx;
}
.address-edit .default-address-seting .select-btn,
.address-edit .default-address-seting .selected-btn{
	margin-top:20rpx;
}
.address-edit .confirm-btn{
	width:330rpx;height:80rpx;line-height:80rpx;font-size:36rpx;letter-spacing:36rpx;text-align:center;text-indent:36rpx;color:#fff;
	background:#6ac4a6;border-radius:6px;margin:50rpx auto 30rpx;
}


















