.submit-results .error{
	width:451rpx;margin:120rpx auto 0;
}
.submit-results .error .title{
	line-height:46rpx;min-height:46rpx;font-size:36rpx;text-align:center;color:#333;
	padding-bottom:27rpx;
}
.submit-results .service-info::before{
	content:"";display:block;margin:0 auto 27rpx;width:135rpx;height:165rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:135rpx 165rpx;
}
.submit-results .service-info view{
	line-height:38rpx;color:#999;font-size:20rpx;text-align:center;display:block;
}
.submit-results .service-info view text{
	color:#f49f37;font-weight:900;
}
.submit-results .back-list{
	width:268rpx;height:74rpx;line-height:74rpx;font-size:30rpx;color:#fff;text-align:center;
	border-radius:6px;background:#f49f37;margin:27rpx auto 0;
}