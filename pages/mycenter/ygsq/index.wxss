.my-address{
	height:50rpx;padding-top:37rpx;
}
.my-address .title{
	height:50rpx;line-height:50rpx;font-size:34rpx;color:#333;text-align:center;
}



.address-edit,
.info-box{
	margin:20rpx 27rpx;background:#fff;border-radius:18rpx;padding:27rpx 27rpx 40rpx;
}
.address-edit .input-box,
.address-edit .area-select-bar,
.address-edit .default-address-seting,
.address-edit .zd-select-bar{
	height:72rpx;overflow:hidden;
	padding:18rpx 0;
}
.address-edit .input-box::before,
.address-edit .area-select-bar::before,
.address-edit .zd-select-bar::before,
.address-edit .zd-address-detail::before{
	content:attr(data-tit);float:left;line-height:72rpx;font-size:26rpx;color:#999;
	text-align:right;width:150rpx;
}
.address-edit .input-box input{
	float:left;width:470rpx;height:70rpx;line-height:70rpx;border:1px solid #ddd;border-radius:4px;background:#fbfbfb;
	padding:0 10rpx;font-size:26rpx;color:#666;
}
.address-edit .area-select-bar .area-text-box,
.address-edit .zd-select-bar .zd-text-box{
	float:left;font-size:26rpx;color:#666;line-height:72rpx;height:72rpx;padding-left:10rpx;
	width:470rpx;
}
.address-edit .area-select-bar .area-text-box::after,
.address-edit .zd-select-bar .zd-text-box::after{
	content:"";float:right;width:18rpx;height:33rpx;margin:19rpx 10rpx 0 0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:18rpx 33rpx;
}
.address-edit .zd-address-detail{
	min-height:72rpx;overflow:hidden;
	padding:18rpx 0;
}
.address-edit .zd-address-detail .text{
	float:left;font-size:26rpx;color:#666;line-height:40rpx;min-height:40rpx;padding-left:10rpx;
	width:470rpx;margin:16rpx 0;
}

.address-edit .btn-bar{
	height:70rpx;padding:27rpx 0 0;
}
.address-edit .btn-bar .submit-address-btn{
	width:396rpx;height:70rpx;line-height:70rpx;background:#6ac4a6;border-radius:4px;color:#fff;text-align:center;
	font-size:30rpx;margin:0 auto;
}


.info-box .check-info{
	min-height:72rpx;overflow:hidden;
	padding:18rpx 0;
}
.info-box .check-info::before{
	content:attr(data-tit);float:left;line-height:72rpx;font-size:26rpx;color:#999;
	text-align:right;width:150rpx;
}
.info-box .check-info text{
	float:left;font-size:26rpx;color:#666;line-height:40rpx;min-height:40rpx;padding-left:10rpx;
	width:470rpx;margin:16rpx 0;
}
.info-box .btn-bar{
	height:70rpx;padding:27rpx 0 0;
}
.info-box .btn-bar .shenhezhong,
.info-box .btn-bar .shenhetongguo{
	width:396rpx;height:70rpx;line-height:70rpx;background:#ccc;border-radius:4px;color:#fff;text-align:center;
	font-size:30rpx;margin:0 auto;
}
.info-box .btn-bar .shenhetongguo{
	background:#49b355;
}



