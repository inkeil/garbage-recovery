/**index.wxss**/
.swiper-box{
	height:290rpx;background:#fff;padding:12rpx 27rpx 27rpx;
}
swiper{
	width:696rpx;height:290rpx;border-radius:18rpx;overflow:hidden;
}
.slide-image{
	width:696rpx;height:290rpx;
}
.month-ranking{
	width:750rpx;height:420rpx;background:#fff;border-radius:18rpx 18rpx 0 0;overflow:hidden;margin-top:25rpx;
	padding:27rpx 0;
}
.month-ranking-title{
	height:42rpx;line-height:42rpx;padding:16rpx 0 0 69rpx;font-size:30rpx;position:relative;
}
.month-ranking-title::before{
	content:"";position:absolute;display:block;width:38rpx;height:58rpx;left:27rpx;top:0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:100%;
}
.month-ranking-title::after{
	content:attr(data-after);font-size:22rpx;color:#888;padding-left:12rpx;
}
.month-ranking-list{
	padding:22rpx 0;height:215rpx;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.month-ranking-list .item-group{
	width:1050rpx;height:215rpx;padding-left:27rpx;
}
.month-ranking-list .item{
	float:left;width:210rpx;height:215rpx;position:relative;
}
.month-ranking-list .item .img{
	position:absolute;width:87rpx;height:87rpx;border:3px solid #fff;border-radius:50%;overflow:hidden;
	left:42rpx;top:0;z-index:2;
}
.month-ranking-list .item .con-box{
	height:160rpx;width:183rpx;position:absolute;left:0;top:54rpx;z-index:1;
	background:#f1f1f1;border-radius:14rpx;overflow:hidden;
}
.month-ranking-list .item .con-box .info-box{
	width:150rpx;height:52rpx;margin:48rpx auto 0 auto;
}
.month-ranking-list .item .con-box .info-box view{
	height:26rpx;line-height:26rpx;font-size:20rpx;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;
	text-align:center;
}
.month-ranking-list .item .con-box .info-box .name{
	font-size:18rpx;color:#999;
}
.month-ranking-list .item .con-box .No1::before,
.month-ranking-list .item .con-box .No2::before,
.month-ranking-list .item .con-box .No3::before{
	content:"";width:33rpx;height:50rpx;float:left;background:#ccc;margin:1rpx 5rpx 0 0;
	background:url(data:image/png;base64,) no-repeat;
	background-size:100%;
}
.month-ranking-list .item .con-box .No2::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:100%;
}
.month-ranking-list .item .con-box .No3::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:100%;
}
.month-ranking-list .item .con-box .No1 view,
.month-ranking-list .item .con-box .No2 view,
.month-ranking-list .item .con-box .No3 view{
	text-align:left;
}

.month-ranking-list .item .con-box .income{
	color:#ed4636;height:52rpx;line-height:52rpx;font-size:28rpx;text-align:center;
}

.my-income-box{
	height:82rpx;padding:22rpx 77rpx 22rpx 27rpx;margin:22rpx 27rpx 0;border-top:1px solid #e9e9e9;
	position:relative;
}
.my-income-box::before{
	content:"";display:block;width:18rpx;height:33rpx;position:absolute;right:27rpx;top:50%;margin-top:-24rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:18rpx 33rpx;
}
.my-income-box .info{
	height:34rpx;line-height:34rpx;font-size:30rpx;color:#ed4636;
}
.my-income-box .info::before{
	content:"";float:left;width:29rpx;height:31rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:100%;margin-right:12rpx;
}
.my-income-box .info::after{
	content:attr(data-after);float:left;font-size:22rpx;color:#333;
}
.my-income-box .tips{
	height:40rpx;line-height:40rpx;font-size:20rpx;color:#999;
}

.buy-price-box{
	background:#fff;overflow:hidden;margin-top:25rpx;
	padding:27rpx;
	padding-bottom:27rpx;
}
.buy-price-box .title{
	height:44rpx;line-height:44rpx;font-size:30rpx;color:#333;
}
.buy-price-box .title::before{
	content:"";width:45rpx;height:43rpx;float:left;margin-right:12rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:100%;
}
.buy-price-box .search-box{
	margin-top:18rpx;height:70rpx;border:1px solid #dad9d9;border-radius:4px;
	background:url(data:image/jpeg;base64,) no-repeat;
	background-color:#fbfbfb;
	background-size:38rpx 38rpx;
	background-position:639rpx center;
}
.buy-price-box .search-box input{
	border:0;height:70rpx;line-height:70rpx;padding:0 74rpx 0 12rpx;font-size:26rpx;color:#999;
}
.buy-price-box .item-box{
	margin-top:30rpx;height:154rpx;border-bottom:1px solid #e9e9e9;overflow:hidden;
}
.buy-price-box .item-box .img{
	float:left;width:124rpx;height:124rpx;border:1px solid #ddd;margin-right:20rpx;
}
.buy-price-box .item-box .con{
	float:left;
}
.buy-price-box .item-box .con .name{
	height:62rpx;line-height:62rpx;font-size:28rpx;color:#333;
}
.buy-price-box .item-box .con .prices{
	height:62rpx;overflow:hidden;
}
.buy-price-box .item-box .con .prices view{
	float:left;width:180rpx;overflow:hidden;height:30rpx;line-height:32rpx;margin:16rpx 0;
	font-size:22rpx;color:#666;overflow:hidden;font-weight:300;
}
.buy-price-box .item-box .con .prices view::before{
	content:"";float:left;width:30rpx;height:30rpx;margin-right:6rpx;
}
.buy-price-box .item-box .con .prices .lie::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:30rpx 30rpx;
}
.buy-price-box .item-box .con .prices .zhong::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:30rpx 30rpx;
}
.buy-price-box .item-box .con .prices .you::before{
	background:url(data:image/png;base64,) no-repeat;
	background-size:30rpx 30rpx;
}

















