
/**app.wxss**/
page{
	background:#f1f2f6;
}
.container {
  height: 100%;
} 
.auth-win{
	position:fixed;left:0;top:0;width:100%;height:100%;background:#f1f2f6;z-index:999;
}
.auth-win .con{
	width:488rpx;height:244rpx;background:#fff;border-radius:18rpx;overflow:hidden;
	padding:30rpx 50rpx;
	position:absolute;left:50%;top:40%;margin:-152rpx 0 0 -294rpx;
}
.auth-win .con .title{
	padding:0 18rpx;height:42rpx;line-height:42rpx;font-size:32rpx;color:#333;
}
.auth-win .con .text{
	color:#777;font-size:28rpx;padding:10rpx 18rpx;line-height:36rpx;
}
.auth-win .con .btn{
	margin:20rpx 0;height:72rpx;line-height:72rpx;background:#51b337;color:#fff;font-size:36rpx;
	border:none;border-radius:8rpx;
}
.version-low-info{
	height:90rpx;line-height:90rpx;text-align:center;color:#666;font-size:32rpx;
	padding-top:37rpx;
}

.select-btn,.selected-btn{
	float:left;width:72rpx;height:32rpx;border-radius:20rpx;background:#6ac4a6;overflow:hidden;
	padding:4rpx;
}
.select-btn{background:#ccc;}
.select-btn::before,
.selected-btn::before{
	content:"";float:left;width:32rpx;height:32rpx;border-radius:50%;background:#fff;
}
.selected-btn::before{float:right;}

/*评价五角星选择器*/

.pingjia-box{
	position:fixed;left:0;top:0;width:100%;height:100%;background:rgba(0,0,0,0.3);z-index:9999;
}
.pingjia-box .con{
	width:400rpx;min-height:100rpx;background:#fff;position:absolute;left:50%;top:50%;
	margin:-90rpx 0 0 -240rpx;padding:40rpx;border-radius:18rpx;
}
.pingjia-box .con .title{
	height:60rpx;line-height:40rpx;font-size:32rpx;color:#999;text-align:center;
}
.pingjia-box .con .select-bar{
	width:400rpx;height:60rpx;
	padding:10rpx 0;
	display: flex;display: -webkit-flex;
	align-items:center;justify-content:center;
}

.pingjia-box .con .select-bar .star_1,
.pingjia-box .con .select-bar .star_2{
	width:64rpx;height:60rpx;margin:0 8rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size:64rpx 60rpx;
}
.pingjia-box .con .select-bar .star_2{
	background:url(data:image/png;base64,) no-repeat;
	background-size:64rpx 60rpx;
}
.pingjia-box .con .btn-bar{
	overflow:hidden;padding-top:27rpx;
	display: flex;display: -webkit-flex;
	align-items:center;justify-content:center;
}
.pingjia-box .con .btn-bar .confirm-btn,
.pingjia-box .con .btn-bar .cancel-btn{
	width:180rpx;height:60rpx;line-height:60rpx;background:#50be84;color:#fff;
	text-align:center;border-radius:4px;overflow:hidden;font-size:32rpx;margin:0 15rpx;
}
.pingjia-box .con .btn-bar .cancel-btn{background:#ccc;float:right;}